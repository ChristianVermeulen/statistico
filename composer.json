{
    "name": "fieg/statistico",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
      "php": ">=5.5"
    },
    "require-dev": {
        "ext-redis": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
      "psr-0": {
        "Fieg": "src/"
      },
      "psr-4": {
        "Fieg\\Statistico\\": "src/Fieg/Statistico/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Fieg\\Statistico\\Tests\\": "tests/Fieg/Statistico/"
      }
    },
    "suggest": {
      "ext-redis": "Allows redis support"
    }
}
